\\ The area of a triangle.
#include<stdio.h>
#include<math.h>
main()
{
    float a,b,c,s,area;
    printf("Enter the length of the three sides of a triangle: ");
    scanf("%f %f %f",&a, &b, &c);
    s=(a+b+c)/2;
    area=sqrt(s*(s-a)*(s-b)*(s-c));
    printf("\nThe area of the triangle is %.2f",area);
}
